.session_wrapper
  .signup__header
    %h1.signup__header__logo
      %a{href: "/"}
        %img{height: "49px", src: "/fmarket_logo_red.svg", width: "185px"}/
    %nav#nav.progress-bar.signup__header__bar
      %ol
        %li
          会員情報
          .progress-status__first.active.let_active
        %li
          電話番号認証
          .progress-status.active.let_active
        %li.active
          お届け先住所入力
          .progress-status.active
        %li
          支払い方法
          .progress-status
        %li
          完了
          .progress-status__last
  .session_wrapper
    .single_container
      .session__main
        .session__main__container
          %h2.session__main__container__title お届け先住所入力
          .session__main__container__box
              .session__main__container__formGroup
                %label{for: "nickname"}
                  郵便番号
                  %span.form__require 必須
                %input.input__default{placeholder: "例）123-4567", type: "text"}/
              .session__main__container__formGroup
                %label{for: "nickname"}
                  都道府県
                  %span.form__require 必須
                .session__main__container__formGroup__sub
                  %select.select__wap{name: "pref"}
                    %option{value: ""} 選択してください
                    %option{selected: "selected", value: "北海道"}
                      北海道
                    %option{value: "青森県"} 青森県
                    %option{value: "岩手県"} 岩手県
                    %option{value: "宮城県"} 宮城県
